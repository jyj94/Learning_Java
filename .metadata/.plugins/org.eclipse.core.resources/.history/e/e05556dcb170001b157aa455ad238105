package operator;

public class operatorClass {
	public static void main(String[] args) {

		int num1 = 5;
		int num2 = 6;
		//단항 연산자
		System.out.println(++num1);
		System.out.println(num2++);
		System.out.println(num2);
		//이항 연산자
		//+ 연산자
		System.out.println(num1 + num2);
		//- 연산자
		System.out.println(num2 - num1);
		//논리 연산자
		System.out.println(5 > 3 && 3 > 2);
	}
}
